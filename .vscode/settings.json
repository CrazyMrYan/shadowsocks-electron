{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": {
    "titleBar.inactiveForeground": "#e7e7e799",
    "titleBar.activeForeground": "#e7e7e7",
    "tab.activeBorder": "#aa0000"
  },
  "files.exclude": {},
  "colorTabs.config": [
    {
      "regex": "main/core\\.*",
      "color": "#aa0000",
      "label": "CORE"
    },
    {
      "regex": "main/\\.*",
      "color": "#ff7f50",
      "label": "CORE"
    },
    {
      "regex": "renderer/\\.*",
      "color": "#7fff00",
      "label": "WEB"
    },
  ],
  "window.title": "~[CORE]~${separator}${separator}${separator}${separator}${separator}${separator}${separator}${dirty}${activeEditorShort}${separator}${rootName}${separator}${appName}"
}
